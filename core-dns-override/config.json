{
  "name": "Core DNS Override",
  "version": "dev",
  "slug": "coredns-fix",
  "description": "Periodically ensure the DNS container isn't using Cloudflare",
  "url": "https://github.com/bentasker/HomeAssistantAddons/tree/master/core-dns-override",
  "advanced": true,
  "startup": "services",
  "ingress": true,
  "ingress_port": 0,
  "ingress_stream": true,
  "panel_icon": "mdi:console",
  "panel_title": "DNSFix",
  "homeassistant": "0.92.0b2",
  "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
  "hassio_api": true,
  "hassio_role": "manager",
  "services": ["mysql:want", "mqtt:want"],
  "homeassistant_api": true,
  "host_network": true,
  "uart": true,
  "usb": true,
  "gpio": true,
  "audio": true,
  "apparmor": false,
  "host_dbus": true,
  "stdin": true,
  "docker_api": true,
  "privileged": ["NET_ADMIN", "SYS_ADMIN", "SYS_RAWIO", "SYS_TIME", "SYS_NICE"],
  "devices": ["/dev/mem"],
  "map": [
    "config:rw",
    "ssl:rw",
    "addons:rw",
    "media:rw",
    "share:rw",
    "backup:rw"
  ],
  "journald": true
}
